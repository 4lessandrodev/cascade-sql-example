{
  "name": "template",
  "version": "1.0.0",
  "description": "Template to start a new app using nestjs and graphql",
  "author": "4lessandrodev",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "env-cmd --silent -f ./.env nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "IS_TESTING=true jest --runInBand --passWithNoTests --silent",
    "test:verbose": "IS_TESTING=true jest --runInBand --passWithNoTests",
    "test:mutation": "IS_TESTING=true stryker run",
    "test:watch": "IS_TESTING=true jest --watch",
    "test:cov": "IS_TESTING=true jest --coverage",
    "test:debug": "IS_TESTING=true node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "IS_TESTING=true jest --config ./jest-e2e.json",
    "prepare": "husky install",
    "postinstall": "husky install",
    "generate": "yarn plop --plopfile ./generator/plopfile.js",
    "postgenerate": "eslint \"{src,tests}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "8.0.11",
    "@nestjs/core": "8.0.11",
    "@nestjs/graphql": "9.0.6",
    "@nestjs/mongoose": "^8.0.1",
    "@nestjs/platform-express": "8.0.11",
    "@nestjs/typeorm": "^8.0.2",
    "apollo-server-express": "3.4.0",
    "env-var": "^7.0.1",
    "graphql": "15.6.1",
    "module-alias": "^2.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "7.3.1",
    "types-ddd": "^2.7.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "8.0.7",
    "@stryker-mutator/core": "^5.4.1",
    "@stryker-mutator/jest-runner": "^5.4.1",
    "@stryker-mutator/typescript-checker": "^5.4.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "16.10.3",
    "@types/redis": "^2.8.32",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.0.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.0",
    "jest": "27.2.5",
    "lint-staged": "11.2.0",
    "plop": "^2.7.4",
    "prettier": "^2.3.2",
    "stryker": "^0.35.1",
    "stryker-cli": "^1.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "4.4.4"
  },
  "_moduleAliases": {
    "@modules": "./dist/modules",
    "@config": "./dist/config",
    "@shared": "./dist/shared",
    "@": "./dist"
  }
}
