{
  "name": "template",
  "version": "1.0.0",
  "description": "Template to start a new app using nestjs and graphql",
  "author": "4lessandrodev",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "^16.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "env-cmd --silent -f ./.env nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "IS_TESTING=true jest --runInBand --passWithNoTests --silent",
    "test:verbose": "IS_TESTING=true jest --runInBand --passWithNoTests",
    "test:mutation": "IS_TESTING=true stryker run",
    "test:watch": "IS_TESTING=true jest --watch",
    "test:cov": "IS_TESTING=true jest --coverage",
    "test:debug": "IS_TESTING=true node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "IS_TESTING=true jest --config ./jest-e2e.config.json --detectOpenHandles --watchAll=false --forceExit --silent",
    "prepare": "husky install",
    "postinstall": "husky install",
    "generate": "yarn plop --plopfile ./generator/plopfile.js",
    "postgenerate": "eslint \"{src,tests}/**/*.ts\" --fix",
    "generate:gql": "graphql-codegen"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.11",
    "@nestjs/core": "^8.0.11",
    "@nestjs/graphql": "^9.0.6",
    "@nestjs/mongoose": "^8.0.1",
    "@nestjs/platform-express": "^8.0.11",
    "@nestjs/typeorm": "^8.0.2",
    "apollo-server-express": "^3.4.0",
    "env-var": "^7.0.1",
    "graphql": "^15.6.1",
    "module-alias": "^2.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.1",
    "types-ddd": "^2.7.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.2",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-resolvers": "^2.4.0",
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.0.7",
    "@stryker-mutator/core": "^5.5.0",
    "@stryker-mutator/jest-runner": "^5.5.0",
    "@stryker-mutator/typescript-checker": "^5.5.0",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.10.3",
    "@types/redis": "^2.8.32",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.1",
    "git-commit-msg-linter": "^3.2.8",
    "graphql-request": "^3.6.1",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.0",
    "plop": "^2.7.6",
    "prettier": "^2.3.2",
    "stryker": "^0.35.1",
    "stryker-cli": "^1.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.4.4"
  },
  "_moduleAliases": {
    "@modules": "./dist/modules",
    "@config": "./dist/config",
    "@shared": "./dist/shared",
    "@app": "./dist",
    "@": "./dist"
  }
}